{
  "name": "@bundlr-network/client",
  "version": "0.11.0-a1",
  "description": "JS client for Bundlr nodes",
  "repository": "https://github.com/Bundlr-Network/js-client.git",
  "author": "Bundlr Network",
  "license": "Apache-2.0",
  "main": "./build/cjs/index.js",
  "browser": "./build/esm/web/index.js",
  "types": "./build/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/cjs/index.d.ts",
      "import": "./build/cjs/index.js",
      "require": "./build/esm/index.js",
      "node": "./build/cjs/index.js"
    },
    "./node": {
      "types": "./build/cjs/node/index.d.ts",
      "import": "./build/esm/node/index.js",
      "require": "./build/cjs/node/index.js",
      "default": "./build/cjs/node/index.d.ts"
    },
    "./web": {
      "types": "./build/esm/web/index.d.ts",
      "import": "./build/esm/web/index.js",
      "require": "./build/cjs/web/index.js",
      "default": "./build/esm/web/index.d.ts"
    },
    "./bundle": {
      "types": "./build/esm/index.d.ts",
      "default": "./build/bundle.js"
    },
    "./*": {
      "types": "./*.d.ts",
      "default": "./*.js"
    }
  },
  "files": [
    "build/**/*.js",
    "build/**/*.js.map",
    "build/**/*.ts"
  ],
  "scripts": {
    "dev:build": "rm -rf build && (tsc -p tsconfig.json && chmod +x ./build/cjs/node/cli.js) & (tsc -p esm.tsconfig.json && chmod +x ./build/esm/node/cli.js)",
    "prod:build": "yarn run dev:build && webpack",
    "lint:check": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "watch": "tsc --watch -p tsconfig.json & webpack --watch",
    "webpack:analyze": "webpack --profile --json /tmp/wp.json && webpack-bundle-analyzer /tmp/wp.json"
  },
  "bin": {
    "bundlr": "./build/cjs/node/cli.js",
    "bundlr-esm": "./build/esm/node/cli.js"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@solana/web3.js": "^1.36.0",
    "@supercharge/promise-pool": "^2.1.0",
    "algosdk": "^1.13.1",
    "aptos": "=1.3.14",
    "arbundles": "^0.9.0-a1",
    "arweave": "=1.11.8",
    "async-retry": "^1.3.3",
    "axios": "^0.25.0",
    "base64url": "^3.0.1",
    "bignumber.js": "^9.0.1",
    "bn.js": "^5.2.1",
    "bs58": "^5.0.0",
    "commander": "^8.2.0",
    "csv": "5.5.3",
    "ethers": "=5.7.2",
    "inquirer": "^8.2.0",
    "js-sha256": "^0.9.0",
    "mime-types": "^2.1.34",
    "near-api-js": "^0.44.2",
    "near-seed-phrase": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@manahippo/aptos-wallet-adapter": "^0.3.6",
    "@solana/wallet-adapter-base": "^0.9.2",
    "@types/async-retry": "^1.4.3",
    "@types/base64url": "^2.0.0",
    "@types/bn.js": "^5.1.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "inspectpack": "^4.7.1",
    "jest": "^27.2.4",
    "longjohn": "^0.2.12",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "serve": "^13.0.2",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
