{
    "name": "@bundlr-network/solana-web",
    "version": "0.1.0-a2",
    "description": "Solana Currency for Bundlr",
    "main": "./esm/web/index.js",
    "types": "./esm/web/index.d.ts",
    "browser": "./esm/web/index.js",
    "author": "JesseTheRobot",
    "license": "MIT",
    "scripts": {
        "dev:build": "rm -rf ./cjs && rm -rf ./esm && tsc -p ./cjs.json && ttsc -p ./esm.json",
        "build": "yarn run dev:build && webpack",
        "watch": "ttsc --watch -p esm.json & ttsc --watch -p cjs.json & webpack --watch"
    },
    "exports": {
        ".": {
            "types": "./esm/web/index.d.ts",
            "import": "./esm/web/index.js",
            "require": "./cjs/web/index.js"
        },
        "./signer": {
            "import": "./esm/web/InjectedSolanaSigner.js",
            "require": "./cjs/web/InjectedSolanaSigner.js"
        },
        "./hexSigner": {
            "import": "./esm/web/HexInjectedSolanaSigner.js",
            "require": "./cjs/web/HexInjectedSolanaSigner.js"
        },
        "./bundle": "./esm/web/bundle.js"
    },
    "files": [
        "cjs/**/*",
        "esm/**/*"
    ],
    "dependencies": {
        "@bundlr-network/client": "^0.10.0-a1",
        "@noble/ed25519": "^1.6.0",
        "@solana/web3.js": "^1.36.0",
        "async-retry": "^1.3.3",
        "base64url": "^3.0.1",
        "bignumber.js": "^9.0.2",
        "bs58": "^5.0.0"
    },
    "devDependencies": {
        "@solana/wallet-adapter-base": "^0.9.18",
        "inspectpack": "^4.7.1",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2"
    }
}
