{
    "name": "@bundlr-network/near-web",
    "version": "0.0.1-rc2",
    "description": "Near Currency for Bundlr",
    "main": "index.js",
    "author": "JesseTheRobot",
    "license": "MIT",
    "scripts": {
        "dev:build": "rm -rf ./cjs && rm -rf ./esm && tsc -p ./cjs.json && ttsc -p ./esm.json",
        "build": "yarn run dev:build && webpack",
        "watch": "ttsc --watch -p esm.json & ttsc --watch -p cjs.json & webpack --watch"
    },
    "exports": {
        ".": {
            "import": "./esm/web/index.js",
            "require": "./cjs/web/index.js"
        },
        "./curve25519": {
            "import": "./esm/node/curve25519.js",
            "require": "./cjs/node/curve25519.js"
        },
        "./signer": {
            "import": "./esm/node/NearSigner.js",
            "require": "./cjs/node/NearSigner.js"
        },
        "./bundle": "./esm/web/bundle.js"
    },
    "files": [
        "cjs/**/*",
        "esm/**/*"
    ],
    "dependencies": {
        "@bundlr-network/client": "^0.8.0-rc2",
        "@noble/ed25519": "^1.6.0",
        "bignumber.js": "^9.0.2",
        "bn.js": "^5.2.0",
        "bs58": "^5.0.0",
        "js-sha256": "^0.9.0",
        "near-api-js": "^0.44.2"
    },
    "devDependencies": {
        "inspectpack": "^4.7.1",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2"
    }
}